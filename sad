local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local Window = Library:NewWindow("Script")
 
local Tab = Window:NewSection("Credits: TGMANKASKE")

Tab:CreateButton("Discord Group", function()

local player = game.Players.LocalPlayer
local UserInputService = game:GetService("UserInputService")
local savedPositionValue = Instance.new("Vector3Value")
savedPositionValue.Name = "SavedPosition"
savedPositionValue.Value = Vector3.new(0, 0, 0)
savedPositionValue.Parent = player

local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.P then
        savedPositionValue.Value = humanoidRootPart.Position
        print("Posição salva:", savedPositionValue.Value)
    end
end)


end)



Tab:CreateButton("Discord Group", function()

local player = game.Players.LocalPlayer
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")

local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local savedPositionValue = player:WaitForChild("SavedPosition")

local moving = false
local hoverHeight = 30 -- altura para subir
local speed = 50       -- velocidade em studs por segundo

local function createTween(targetCFrame, duration)
    local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear)
    return TweenService:Create(humanoidRootPart, tweenInfo, {CFrame = targetCFrame})
end

local function gotoPosition(targetPos)
    if moving then return end
    moving = true

    local startPos = humanoidRootPart.Position
    local upPos = Vector3.new(startPos.X, startPos.Y + hoverHeight, startPos.Z)
    local hoverCFrame = CFrame.new(upPos)
    local targetHoverPos = Vector3.new(targetPos.X, upPos.Y, targetPos.Z)
    local targetHoverCFrame = CFrame.new(targetHoverPos)
    local targetCFrame = CFrame.new(targetPos)

    local upDist = (upPos - startPos).Magnitude
    local horizDist = (targetHoverPos - upPos).Magnitude
    local downDist = (targetPos - targetHoverPos).Magnitude

    local upTween = createTween(hoverCFrame, upDist / speed)
    local horizTween = createTween(targetHoverCFrame, horizDist / speed)
    local downTween = createTween(targetCFrame, downDist / speed)

    upTween:Play()
    upTween.Completed:Wait()

    horizTween:Play()
    horizTween.Completed:Wait()

    downTween:Play()
    downTween.Completed:Wait()

    moving = false
end

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.O then
        local targetPos = savedPositionValue.Value
        if targetPos.Magnitude > 0 then
            gotoPosition(targetPos)
        else
            warn("Nenhuma posição salva.")
        end
    end
end)


end)
